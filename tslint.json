{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "member-access": [true, "no-public"],
    "no-console": [true, "warning"],
    "no-var-requires": false
  }
}
